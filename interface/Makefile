	@$(QMAKE) -o Makefile interface.pro

dist: 
	@mkdir -p .tmp/interface && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/interface/ && $(COPY_FILE) --parents spg.ui.h .tmp/interface/ && ( cd `dirname .tmp/interface` && $(TAR) interface.tar interface && $(GZIP) interface.tar ) && $(MOVE) `dirname .tmp/interface`/interface.tar.gz . && $(DEL_FILE) -r .tmp/interface

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

