main.o: main.cpp ../include/Controlador.h \
		../interface/spg.h \
		../include/Pagina.h \
		../include/StructPresenca.h \
		../include/StructAssociacao.h

spg.h: spg.ui ../include/Controlador.h \
		../include/Pagina.h \
		../include/StructPresenca.h \
		../include/StructAssociacao.h
	$(UIC) spg.ui -o spg.h

spg.cpp: spg.h spg.ui spg.ui.h ../include/Controlador.h \
		../include/Pagina.h \
		../include/StructPresenca.h \
		../include/StructAssociacao.h
	$(UIC) spg.ui -i spg.h -o spg.cpp

spg.o: spg.cpp spg.ui.h \
		spg.h

moc_spg.o: moc_spg.cpp  spg.h ../include/Controlador.h \
		../include/Pagina.h \
		../include/StructPresenca.h \
		../include/StructAssociacao.h

moc_spg.cpp: $(MOC) spg.h
	$(MOC) spg.h -o moc_spg.cpp

